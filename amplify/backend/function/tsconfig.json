{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Amplify Lambda",
  "compilerOptions": {
    "target": "es2020",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
